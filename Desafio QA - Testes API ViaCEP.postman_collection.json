  "item": [
    {
      "name": "CT01 - FB - CEP Válido",
      "item": [
        {
          "name": "CT01 - Consultar CEP válido",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"CT01 - Status code é 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"CT01 - Resposta está no formato JSON\", function () {",
                  "    pm.response.to.be.json;",
                  "});",
                  "",
                  "pm.test(\"CT01 - Campo 'cep' retorna '01001-000'\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.cep).to.eql(\"01001-000\");",
                  "});",
                  "",
                  "pm.test(\"CT01 - Campo 'logradouro' retorna 'Praça da Sé'\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.logradouro).to.eql(\"Praça da Sé\");",
                  "});",
                  "",
                  "pm.test(\"CT01 - Campo 'localidade' retorna 'São Paulo'\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.localidade).to.eql(\"São Paulo\");",
                  "});",
                  "",
                  "pm.test(\"CT01 - Campo 'uf' retorna 'SP'\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.uf).to.eql(\"SP\");",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
