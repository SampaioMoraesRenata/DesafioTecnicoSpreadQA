              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"CT03 - Status code indica erro (400)\", function () {",
                  "    pm.response.to.have.status(400);",
                  "});",
                  "",
                  "pm.test(\"CT03 - Resposta indica requisição inválida (Bad Request)\", function () {",
                  "    var responseText = pm.response.text();",
                  "    pm.expect(responseText).to.include(\"400\");",
                  "});",
                  "",
                  "pm.test(\"CT03 - Resposta não está no formato JSON\", function () {",
                  "    var contentType = pm.response.headers.get(\"Content-Type\");",
                  "    pm.expect(contentType).to.not.include(\"application/json\");",
                  "});"
